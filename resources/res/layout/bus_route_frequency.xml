<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:id="@+id/frequency_route_body" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <androidx.appcompat.widget.Toolbar android:id="@+id/frequency_toolbar" android:layout_width="match_parent" android:layout_height="@dimen/action_bar_height" android:title="@string/SCHEDULE" style="@style/ZophopToolbarStyle.WhiteToolBar"/>
    <LinearLayout android:orientation="vertical" android:background="@color/new_activity_color" android:paddingLeft="16dp" android:paddingTop="12dp" android:paddingRight="16dp" android:paddingBottom="12dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="18dp" android:textColor="@color/black_87" android:ellipsize="end" android:id="@+id/frequency_start_stop" android:layout_width="match_parent" android:layout_height="wrap_content" android:maxLines="2" style="@style/mediumFont"/>
        <TextView android:textColor="@color/black_87" android:ellipsize="end" android:id="@+id/tv_schedule_first_last" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="4dp" android:maxLines="1" style="@style/regularFont.NormalText"/>
    </LinearLayout>
    <ListView android:id="@+id/frequency_list" android:paddingBottom="48dp" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="match_parent" android:divider="@null"/>
</LinearLayout>
