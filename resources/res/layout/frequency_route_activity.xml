<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="#e6e4e4" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:id="@+id/frequency_route_body_container" android:layout_width="match_parent" android:layout_height="match_parent">
        <LinearLayout android:orientation="vertical" android:id="@+id/frequency_route_body" android:layout_width="match_parent" android:layout_height="match_parent">
            <ListView android:id="@+id/frequency_list" android:background="@drawable/frequency_list_bg" android:paddingBottom="4dp" android:layout_width="match_parent" android:layout_height="@dimen/frequency_list_height"/>
            <View android:layout_width="match_parent" android:layout_height="12dp"/>
            <include layout="@layout/route_container"/>
        </LinearLayout>
    </FrameLayout>
</LinearLayout>
